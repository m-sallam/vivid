            <input class="input" type="email" title="E-mail"  name="email" placeholder="E-mail">
            <input class="input" type="text" title="Name"  name="name" placeholder="Name">
            <input class="input" type="text" title="country"  name="country" placeholder="Country">            
            <input class="input" type="password" title="Password"  name="password" placeholder="Password">
            <a href="#login_popup">Already registered?</a>
            <button class="btn-primary">Register</button>
