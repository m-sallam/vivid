    <div class="mfp-hide login-popup zoom-anim-dialog" id="login_popup">
        <h2>Welcome</h2>
        <form class="form-horizontal" method="POST" action="/volunteer/login">
            <input class="input-block" type="text" title="username" placeholder="Username" name="username" value="" required autofocus>
            <input class="input-block" type="password" title="Password" placeholder="Password" name="password" required>
            <button class="btn-primary">Login</button>
            <div class="clearfix"></div>
        </form>
    </div>

    <div class="mfp-hide signup-popup zoom-anim-dialog" id="signup_popup">
        <h2>New Account</h2>
        <form class="form-horizontal" method="POST" action="/volunteer/register">
